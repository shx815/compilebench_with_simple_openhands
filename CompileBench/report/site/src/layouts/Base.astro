---
import "../styles/tailwind.css";
import "../styles/global.css";
const { title = 'CompileBench', description = '' } = Astro.props;
const canonicalUrl = `https://www.compilebench.com${Astro.url.pathname}`;
---
<!doctype html>
<html lang="en">
  <head>
    <!-- Google tag (gtag.js) -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=G-FPBGJB67DW"></script>
    <script>
      window.dataLayer = window.dataLayer || [];
      function gtag(){dataLayer.push(arguments);}
      gtag('js', new Date());

      gtag('config', 'G-FPBGJB67DW');
    </script>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title>{title}</title>
    <meta name="description" content={description}>
    <link rel="canonical" href={canonicalUrl} />
    <!-- Open Graph / Facebook -->
    <meta property="og:type" content="website">
    <meta property="og:url" content="https://www.compilebench.com/">
    <meta property="og:title" content={title}>
    <meta property="og:description" content={description}>
    <meta property="og:image" content="https://www.compilebench.com/og-image.png">
    <meta property="og:site_name" content="CompileBench">
    <!-- Twitter -->
    <meta property="twitter:card" content="summary_large_image">
    <meta property="twitter:url" content="https://www.compilebench.com/">
    <meta property="twitter:title" content={title}>
    <meta property="twitter:description" content={description}>
    <meta property="twitter:image" content="https://www.compilebench.com/og-image.png">
    <!-- JSON-LD -->
    <script type="application/ld+json">
      {
        "@context": "https://schema.org",
        "@type": "WebSite",
        "name": "CompileBench",
        "url": "https://www.compilebench.com/",
        "description": "CompileBench evaluates AI models on real-world software builds, testing their ability to handle dependency hell, legacy toolchains, and complex build systems. See how LLMs perform on practical engineering tasks.",
        "inLanguage": "en",
        "image": "https://www.compilebench.com/og-image.png",
        "publisher": {
          "@type": "Organization",
          "name": "Quesma",
          "url": "https://quesma.com/",
          "logo": {
            "@type": "ImageObject",
            "url": "https://www.compilebench.com/assets/logos/quesma.png"
          },
          "sameAs": [
            "https://github.com/QuesmaOrg/CompileBench",
            "https://x.com/QuesmaOrg",
            "https://www.linkedin.com/company/quesma"
          ]
        }
      }
    </script>
    
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Inter:wght@400;500;600;700&family=Orbitron:wght@400&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.2/css/all.min.css" />
    <link rel="icon" href="/assets/logos/quesma.ico" type="image/x-icon" />
    <link rel="icon" href="/assets/logos/quesma.png" type="image/png" />
    <link rel="icon" href="/assets/logos/quesma.svg" type="image/svg+xml" />
    <link rel="apple-touch-icon" href="/assets/logos/quesma.png" />
    <style>
      .bg-striped-placeholder { background-image: repeating-linear-gradient(-45deg, transparent 0, transparent 14px, rgba(100,116,139,0.14) 2px, rgba(100,116,139,0.14) 16px) }
      html { scroll-behavior: smooth }
    </style>
    <slot name="extra_head"></slot>
  </head>
  <body style="background-color: oklch(0.992 0 0)" class="min-h-screen flex flex-col font-sans">
    <nav>
      <div class="max-w-5xl mx-auto px-6 py-3 mt-4 mb-4 flex flex-col md:flex-row items-start md:items-center gap-y-4 md:gap-y-0 md:gap-x-6">
        <div class="inline-block text-center select-none">
          <div class="text-4xl text-slate-800" style="font-family:'Orbitron', ui-sans-serif, system-ui, -apple-system, 'Segoe UI', Arial, 'Noto Sans', sans-serif; font-weight:400;"><a href="/" class="hover:opacity-75">CompileBench</a></div>
          <div class="text-slate-600 leading-tight text-right mt-[-0.15em]"><a href="https://quesma.com/" target="_blank" class="hover:opacity-75" aria-label="Quesma website">by <img src="/assets/logos/quesma-logotype.svg" alt="Quesma" class="inline-block align-middle w-20 text-slate-600" /></a></div>
        </div>
        <div class="md:ml-auto flex items-center gap-x-5">
          <a href="/" class="text-slate-600 hover:text-slate-800 transition-colors">Ranking</a>
          <a href="/about/" class="text-slate-600 hover:text-slate-800 transition-colors">About</a>
          <a href="/tasks/" class="text-slate-600 hover:text-slate-800 transition-colors">Tasks</a>
          <a href="/changelog/" class="text-slate-600 hover:text-slate-800 transition-colors">Changelog</a>
          <a href="https://github.com/QuesmaOrg/CompileBench" target="_blank" aria-label="GitHub repository" class="text-slate-600 hover:text-slate-800 transition-colors"><svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" class="h-6 w-6" aria-hidden="true"><path fill="currentColor" d="M12 0C5.37 0 0 5.37 0 12c0 5.3 3.438 9.8 8.206 11.387.6.113.82-.258.82-.577 0-.285-.01-1.04-.016-2.04-3.338.726-4.042-1.61-4.042-1.61-.546-1.387-1.333-1.758-1.333-1.758-1.09-.744.083-.729.083-.729 1.205.085 1.84 1.237 1.84 1.237 1.07 1.835 2.807 1.305 3.492.997.108-.774.42-1.305.762-1.605-2.665-.304-5.466-1.33-5.466-5.93 0-1.31.47-2.382 1.236-3.222-.124-.303-.536-1.523.117-3.176 0 0 1.008-.322 3.3 1.23.957-.266 1.984-.399 3.005-.404 1.02.005 2.047.138 3.006.404 2.29-1.552 3.296-1.23 3.296-1.23.655 1.653.243 2.873.12 3.176.77.84 1.235 1.912 1.235 3.222 0 4.61-2.807 5.624-5.48 5.921.431.372.823 1.102.823 2.222 0 1.606-.015 2.898-.015 3.293 0 .32.216.694.826.576C20.565 21.796 24 17.296 24 12c0-6.63-5.37-12-12-12z"/></svg></a>
          <a href="https://x.com/QuesmaOrg" target="_blank" rel="noopener noreferrer" aria-label="X (Twitter)" class="text-slate-600 hover:text-slate-800 transition-colors"><i class="fa-brands fa-x-twitter text-[24px]" aria-hidden="true"></i></a>
          <a href="https://www.linkedin.com/company/quesma" target="_blank" rel="noopener noreferrer" aria-label="LinkedIn" class="text-slate-600 hover:text-slate-800 transition-colors"><i class="fa-brands fa-linkedin text-[24px]" aria-hidden="true"></i></a>
        </div>
      </div>
    </nav>
    <main class="flex-1">
      <slot />
    </main>
    <footer class="mt-16 border-t border-slate-200">
      <div class="max-w-5xl mx-auto px-6 py-6 flex items-center justify-between">
        <div class="text-slate-600 text-sm">
          <div class="flex items-center gap-x-1">
            © <a href="https://quesma.com" target="_blank" class="inline-flex items-center hover:text-slate-800 transition-colors"><img src="/assets/logos/quesma-logotype.svg" alt="Quesma" class="inline-block w-20 text-slate-800" /></a><span>Inc.</span><span>2025</span>
          </div>
          <div class="text-slate-400 text-xs mt-1 max-w-lg leading-snug">All product names, logos, and brands (™/®) are the property of their respective owners; they’re used here solely for identification and comparison, and their use does not imply affiliation, endorsement, or sponsorship.</div>
        </div>
        <div class="flex items-center gap-x-3 shrink-0">
          <a href="https://github.com/QuesmaOrg/CompileBench" target="_blank" aria-label="GitHub repository" class="text-slate-600 hover:text-slate-800 transition-colors"><svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" class="h-5 w-5" aria-hidden="true"><path fill="currentColor" d="M12 0C5.37 0 0 5.37 0 12c0 5.3 3.438 9.8 8.206 11.387.6.113.82-.258.82-.577 0-.285-.01-1.04-.016-2.04-3.338.726-4.042-1.61-4.042-1.61-.546-1.387-1.333-1.758-1.333-1.758-1.09-.744.083-.729.083-.729 1.205.085 1.84 1.237 1.84 1.237 1.07 1.835 2.807 1.305 3.492.997.108-.774.42-1.305.762-1.605-2.665-.304-5.466-1.33-5.466-5.93 0-1.31.47-2.382 1.236-3.222-.124-.303-.536-1.523.117-3.176 0 0 1.008-.322 3.3 1.23.957-.266 1.984-.399 3.005-.404 1.02.005 2.047.138 3.006.404 2.29-1.552 3.296-1.23 3.296-1.23.655 1.653.243 2.873.12 3.176.77.84 1.235 1.912 1.235 3.222 0 4.61-2.807 5.624-5.48 5.921.431.372.823 1.102.823 2.222 0 1.606-.015 2.898-.015 3.293 0 .32.216.694.826.576C20.565 21.796 24 17.296 24 12c0-6.63-5.37-12-12-12z"/></svg></a>
          <a href="https://x.com/QuesmaOrg" target="_blank" rel="noopener noreferrer" aria-label="X (Twitter)" class="text-slate-600 hover:text-slate-800 transition-colors"><i class="fa-brands fa-x-twitter text-[20px]" aria-hidden="true"></i></a>
          <a href="https://www.linkedin.com/company/quesma" target="_blank" rel="noopener noreferrer" aria-label="LinkedIn" class="text-slate-600 hover:text-slate-800 transition-colors"><i class="fa-brands fa-linkedin text-[20px]" aria-hidden="true"></i></a>
        </div>
      </div>
    </footer>
    <script src="/js/table-renderer.js" is:inline></script>
  </body>
  </html>


